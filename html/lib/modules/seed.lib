--seed_empty
flags family seed
component seed_empty

--seed_border
flags default true
flags family seed
component seed_border
par seed_w 0.4
modules t t_id
sub reduce z = torus_reduce(z)

--seed_multi
flags default true
flags family seed
component seed_multi
modules seed0 seed_border
modules seed1 seed_border
sub reduce z = torus_reduce(z)
