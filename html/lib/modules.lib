--t_id
flags family t
component t_wrap
sub t_inner z = z
sub t_main  z = z
sub t_outer z = z

--t_wrap
flags default true
flags family t
component t_wrap
zn 1.0 + 0.0i, 0.0 + 0.0i, 1.0 + 0.0i, 0.0 + 0.0i,
sub t_inner z = zn[#0] * z + zn[#1]
sub t_main  z = exp(z)
sub t_outer z = zn[#2] * z + zn[#3]

--t_wrap2
flags family t
component t_wrap
sub t_inner z = z
sub t_main  z = 1.05 * z
sub t_outer z = z



